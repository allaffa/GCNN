{
    "Dataset": {
        "name": "unit_test",
        "format": "unit_test",
        "num_nodes": 10,
        "node_features": {
            "name": ["x","x2","x3"],
            "dim": [1, 1, 1],
            "column_index": [0, 6, 7]
        },
        "graph_features":{
            "name": [ "sum_x_x2_x3"],
            "dim": [1],
            "column_index": [0]
        }
    },
    "NeuralNetwork": {
        "Architecture": {
            "model_type": "PNN",
            "radius": 7,
            "max_neighbours": 4,
            "hidden_dim": 10,
            "num_conv_layers": 5,
            "num_sharedlayers": 2,
            "task_weights": [1.0, 1.0, 1.0, 1.0]
        },
        "Variables_of_interest": {
            "input_node_features": [0],
            "output_names": ["sum_x_x2_x3"],
            "output_index": [0],
            "type": ["graph"],
            "denormalize_output": "True"
        },
        "Training": {
            "num_epoch": 100,
            "perc_train": 0.7,
            "learning_rate": 0.02,
            "batch_size": 32
        }
    }
}